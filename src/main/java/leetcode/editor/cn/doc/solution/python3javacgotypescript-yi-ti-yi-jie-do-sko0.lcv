function minFallingPathSum(grid: number[][]): number {
    const n = grid.length;
    const f: number[][] = Array(n + 1)
        .fill(0)
        .map(() => Array(n).fill(0));
    const inf = 1 << 30;
    for (let i = 1; i <= n; ++i) {
        for (let j = 0; j < n; ++j) {
            let x = inf;
            for (let k = 0; k < n; ++k) {
                if (k !== j) {
                    x = Math.min(x, f[i - 1][k]);
                }
            }
            f[i][j] = grid[i - 1][j] + (x === inf ? 0 : x);
        }
    }
    return Math.min(...f[n]);
}
