function minFallingPathSum(grid: number[][]): number {
    let [f, g, fp] = [0, 0, -1];
    const inf = 1 << 30;
    for (const row of grid) {
        let [ff, gg, ffp] = [inf, inf, -1];
        for (let j = 0; j < row.length; ++j) {
            const s = (j !== fp ? f : g) + row[j];
            if (s < ff) {
                [gg, ff, ffp] = [ff, s, j];
            } else if (s < gg) {
                gg = s;
            }
        }
        [f, g, fp] = [ff, gg, ffp];
    }
    return f;
}
